
Процедура ПриОткрытии()
	
	СправочникСписок.Отбор.Тип.Значение      = Перечисления.ТипыОбъектов.ПланОбмена;
	СправочникСписок.Отбор.Тип.ВидСравнения  = ВидСравнения.НеРавно;
	СправочникСписок.Отбор.Тип.Использование = Истина;
	
КонецПроцедуры

Процедура СправочникСписокПриВыводеСтроки(Элемент, ОформлениеСтроки, ДанныеСтроки)
	
	ОбъектыМетаданныхПриВыводеСтроки(Элемент, ОформлениеСтроки, ДанныеСтроки);
	
КонецПроцедуры

СправочникСписок.Колонки.Добавить("Имя");
СправочникСписок.Колонки.Добавить("Наименование");
СправочникСписок.Колонки.Добавить("Тип");
СправочникСписок.Колонки.Добавить("ПометкаУдаления");

